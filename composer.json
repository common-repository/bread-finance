{
    "name": "bread/woocommercegateway",
    "description": "WooCommerce Gateway Bread",
    "type": "project",
    "require": {
        "sentry/sdk": "2.0",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.5.23",
        "yoast/phpunit-polyfills": "^1.0",
        "rector/rector": "0.18.4"
    },
    "autoload": {
        "psr-4": {
          "Bread_Finance\\Classes\\": "classes/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform-check": false
    }
}
